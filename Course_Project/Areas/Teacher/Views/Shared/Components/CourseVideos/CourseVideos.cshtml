@model List<VideoViewModel>

<table class="table table-striped table-primary custom-table">
    <tbody>
        @foreach (var v in Model)
        {
            <tr>
                <td class="mb-2 rounded-2">
                    <h4 class="fw-bold">
                        عنوان :
                        <label class="fw-bold h5 text-black">@v.Title</label>
                    </h4>
                    <video src="@v.Url" class="w-100 rounded-2" controls></video>
                    <div class="text-center">
                        <input type="button" class="btn btn-outline-warning" value="ویرایش ویدیو" />
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>

